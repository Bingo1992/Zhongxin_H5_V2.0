    <script>
    //  // 初始化资源加载器
    var resList = [
        "img/load_title.png",
        "img/logo_1.png",
        "img/bg.jpg",
        "img/line.png",
        "img/logo_1.png",
        "img/pic_p1.jpg",
        "img/p1_light_1.png",
        "img/p1_light_2.png",
        "img/p1_text_1.png",
        "img/p1_text_2.png",
        "img/p1_text_3.png",
        "img/p1_text_4.png",
        "img/p1_text_5.png"

    ];

    // var lock = false;
    var audio = true;
    // window.onload = function() {
    //     if (lock) {
    //         return;
    //     }
    //     lock = true;
    new mo.Loader(resList, {
            loadType: 1,
            minTime: 200,
            onLoading: function(count, total) {
                var per = parseInt(count / total * 100);
                $('.count').html(per); //loading
                $('.progress-bar span').css('width', per + "%");
            },
            onComplete: function(time) {
                setTimeout(function() {
                    $('.loading').animate({},
                        200,
                        function() {
                            lock = false;
                            window.pageSlide = new mo.Slide({
                                target: $('.slide .page'),
                                lazy: true,
                                effect: 'flip',
                                playTo: 0,
                                circle: false
                            });

                            $('.loading').css('display', 'none');

                        });
                }, 200);
            }
        })
        // }

    // 插入音乐
    var url = "audio.mp3";
    window.myAudio = document.createElement("audio");
    var source = document.createElement("source");
    myAudio.id = "audio";
    source.type = "audio/mpeg";
    source.src = url;
    source.autoplay = "autoplay";
    source.loop = "loop";
    myAudio.appendChild(source);
    myAudio.play();
    $('.slide').one('touchstart', function() {
        if (audio) {
            myAudio.play();
        }
    })


    //播放禁止音乐
    $('.p-music').click(function() {
        if (myAudio.paused) {
            myAudio.play();
            $(this).addClass('p-music-on');
            audio = true;
        } else {
            myAudio.pause();
            audio = false;
            $(this).removeClass('p-music-on');
        }
    });

    //花瓣
   window.onload = function(){
    // var width = document.body.clientWidth;
    // var height = document.body.clientHeight;   
    // $('.flower').css({
    //     'width':width,
    //     'height':height
    // })
        for(var a=0; a<15; a++){
            var ranLeaf = parseInt(Math.random()*4);
            $('.flower').append('<img class="motion-flower" src="img/realLeaf'+(ranLeaf+1)+'.png">')
        }
        var $img = $('.flower').find('img');
        var len = $img.length;
        for(var i=0; i<len; i++){
            var ranWidth = Math.random()*60+30;
            var ranLeft = Math.random()*400+100;
            var time1 = Math.random()*10 + 10;
            var time2 = Math.random()*11;
            // var direction = ['left','right'];
            // if(time2<5){
                // ranDir = direction[0];
                // $img.eq(i).css('left',ranLeft +'px');
                $img.eq(i).css({
                'width':ranWidth+'px',
                'left':ranLeft +'px',
                '-webkit-animation-duration': time1+'s',
                '-webkit-animation-delay': time2+'s'
                });

            // }else{
            //     $img.eq(i).css({
            //     'width':ranWidth+'px',
            //     'right':ranLeft +'px',
            //     '-webkit-animation-duration': time1+'s',
            //     '-webkit-animation-delay': time2+'s'
            //     });

            // }
   }
        
             
        
    }

    //查询按钮
    $('.btn-check').click(function(){
       $('#success-dialog').addClass('show');
        
       $('.btn-confirm').click(function(){
            $('.result-dialog').removeClass('show');
            window.pageSlide.playTo(5);//播放第五页
       });
       $('.btn-reset').click(function(){
            $('.result-dialog').removeClass('show');
       });

    });

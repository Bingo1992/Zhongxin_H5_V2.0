@charset 'UTF-8';
body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,p,th,td,header,hgroup,nav,section,article,aside,footer,menu,button,img,a,s,form ,select,input{
    margin: 0;
    padding: 0;
    outline: 0;
}
html {font-size: 20px;} 
html, body{height: 100%;}     
body {
    font: 14px  'Microsoft Yahei',Tahoma,Helvetica,Arial,sans-serif;
    -webkit-user-select: none;
    color: #333;
    -webkit-text-size-adjust: none;
    -webkit-tap-highlight-color: rgba(255,255,255,0);
    /*background: url(../img/bg.jpg) repeat;*/
}
a{text-decoration: none;}
ul,li{list-style: none;}
img {display: block;max-width: 100%;}
h1,h2,h3,h4,h5,h6{font-weight: normal;}
select,input,button {
	background: 0 0;
	border: 0;
	-webkit-appearance:none;
	appearance:none;
}
.p-music{
    position: absolute;
    top: .5rem;
    right: .5rem;
    z-index: 10;
 
}
.p-music-on{
	-webkit-animation: rotate 3s linear infinite;/* Safari and Chrome */
	animation: rotate 3s linear infinite;/* Safari and Chrome */
}
.p-music img {width: 1.5rem;}
.slide {position:relative; width:100%; height:100%;overflow: hidden;}
.slide .content{width: 100%;height: 100%;margin: 0 auto;}
.slide  .page{
	width: 100%;
	height:100%;
	overflow: hidden;
	background-image: url(../img/bg.jpg);
	background-size: cover;
	text-align: center;
}

.triangle{
	position: absolute;
	bottom: 2rem;
	left: 50%;
	margin-left: -.7rem;
	width: 1.4rem;
	-webkit-animation: translateC_1 2s linear infinite; 
	animation: translateC_1 2s linear infinite;
}
.triangle img{
	width: 1.4rem;
}

/*-----loading-----*/
.loading {
	display: -webkit-box;
	-webkit-box-pack:center;
	-webkit-box-align:center;
	-webkit-box-orient:vertical;
	width: 100%;
	height: 100%;
	position: fixed;
	z-index:110;
	background: #f1f4f7 url(../img/bg.jpg) repeat;
	color: #06274a;
	font-size: .8rem;
}
.progress-bar {
	width: 80%;
	height: 3px;
	overflow: hidden;
	margin: .5rem auto;
	background-color: #ccc;
	position: relative;
}
.progress-bar span {
	position: absolute;
	top: 0;
	left: 0;
	height: 3px;
	background-color: #d32222;
	display: block;
	/*width: 20%;*/
}

.load_title {
	width: 12%;
	top: 8%;
	position: absolute;
	left: 50%;
	margin-left: -6%;
}
.logo_1 {
	width: 30%;
	position: absolute;
	bottom: 8%;
	left: 50%;
	margin-left: -15%;
}


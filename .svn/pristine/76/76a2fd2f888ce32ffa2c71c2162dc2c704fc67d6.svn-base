/*查询结果*/
.result-dialog {
	position: fixed;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	background: url(../img/bg.jpg) repeat;
	display: none;
	z-index: 50;
}

.dialog-title {
	background: url(../img/p5_box_title.png);
    background-size: 100% 100%;
    width: 100%;
    height: 3.2rem;
    box-sizing: border-box;
    padding-top: 1.6rem;
    color: #fff;
    font-size: .8rem;
    text-align: center;
}

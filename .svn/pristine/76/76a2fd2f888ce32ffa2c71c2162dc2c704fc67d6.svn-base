 .p4_box{
	width: 90%;
	margin: 5% auto;
 }
.p4_light {
	position: absolute;
    width: 60%;
    right: -3rem;
    top: -1rem;
    z-index: 20;
 /*------第五屏-----*/
}
.p5_title{
 	color: #06274a;
 	text-align: center;
 	line-height: 1.4rem;
 	padding-top: 3rem;
 }
.p5_title h2{
	font-weight: normal;
	font-size: 1.4rem;
	padding-bottom: .5rem;
}
.p5_title p {
  	font-size: .8rem;
  }
